{
  "homepage": "http://Hrnjez.github.io/eCommerce-ProShop",
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN shoping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hrnjez/eCommerce-ProShop.git"
  },
  "author": "Nemanja Hrnjez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hrnjez/eCommerce-ProShop/issues"
  },
  "homepage": "https://github.com/Hrnjez/eCommerce-ProShop#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.11.8"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "gh-pages": "^3.1.0",
    "nodemon": "^2.0.6"
  }
}
